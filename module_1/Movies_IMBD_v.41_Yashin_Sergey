    "data_23 = data.copy()\n",
    "dir_total = set(data_23.director.str.split('|').sum())\n",
    "winter_list = ['1', '2', '12']\n",
    "data_23['date_list'] = data_23['release_date'].str.split('/')\n",
    "data_23['month'] = data_23.date_list.str[0]\n",
    "\n",
    "\n",
    "data_winter = data_23[(data_23['month'] == winter_list[0]) | \n",
    "                     (data_23['month'] == winter_list[1]) |\n",
    "                     (data_23['month'] == winter_list[2])\n",
    "                     ][['month', 'director']]\n",
    "\n",
    "res = data_winter.groupby(['director']).count().sort_values(['month'])\n",
    "res\n"
